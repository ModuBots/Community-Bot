{
    "name": "community-engagement-bot",
    "version": "1.0.0",
    "description": "A Discord bot for community engagement with games, events, and more",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "discord",
        "bot",
        "community",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "discord.js": "^14.14.1",
        "dotenv": "^16.3.1",
        "mongoose": "^8.0.1",
        "reflect-metadata": "^0.1.13",
        "tsyringe": "^4.8.0"
    },
    "devDependencies": {
        "@types/node": "^20.9.1",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "eslint": "^8.53.0",
        "prettier": "^3.1.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
    }
}
